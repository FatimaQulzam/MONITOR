{
  "name": "vps-monitor",
  "version": "1.0.0",
  "description": "VPS monitoring system with automated coin-based billing and server suspension",
  "main": "Hamza.js",
  "scripts": {
    "start": "node Hamza.js",
    "dev": "nodemon Hamza.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pm2:start": "pm2 start Hamza.js --name vps-monitor",
    "pm2:stop": "pm2 stop vps-monitor",
    "pm2:restart": "pm2 restart vps-monitor",
    "pm2:logs": "pm2 logs vps-monitor",
    "logs": "tail -f coin-monitor.log"
  },
  "keywords": [
    "vps",
    "monitoring",
    "billing",
    "pterodactyl",
    "automation",
    "cron",
    "server-management"
